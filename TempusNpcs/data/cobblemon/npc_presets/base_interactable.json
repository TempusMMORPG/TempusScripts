{
    "resourceIdentifier": "cobblemon:interactable",
    "canDespawn": false,
    "isMovable": false,
    "isInvulnerable": true,
    "isLeashable": false,
    "allowProjectileHits": false,
    "ai": [
      {
        "type": "apply_presets",
        "presets": [
          "cobblemon:looks",
          "cobblemon:chats"
        ]
      }  
    ],
    "config": [
      {
        "variableName": "node_health",
        "displayName": "Node Health",
        "description": "The default health of the node",
        "type": "NUMBER",
        "defaultValue": "31"
      },
      {
        "variableName": "node_cooldown",
        "displayName": "Node Cooldown",
        "description": "The cooldown of the node in seconds between hits (0.1 or more)",
        "type": "NUMBER",
        "defaultValue": "1"
      },
      {
        "variableName": "reward_pool",
        "displayName": "Reward Pool Script",
        "description": "The script used to determine the reward pool",
        "type": "TEXT",
        "defaultValue": "cobblemon:iron_node_reward"
      },
      {
        "variableName": "interactable_script",
        "displayName": "Interactable Script",
        "description": "The script used to determine the type of interactable: e.g node",
        "type": "TEXT",
        "defaultValue": "cobblemon:node"
      }
    ],
    "hideNameTag": true
}