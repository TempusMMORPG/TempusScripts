{
    "hitbox": {
      "width": 1.0,
      "height": 1.0
    },
    "canDespawn": false,
    "isMovable": false,
    "isInvulnerable": true,
    "isLeashable": false,
    "allowProjectileHits": false,
    "ai": [
      {
        "type": "apply_presets",
        "presets": [
          "cobblemon:looks",
          "cobblemon:chats"
        ]
      }  
    ],
    "interaction": {
      "type": "custom_script",
      "script": "q.run_script('cobblemon:iron_node');"
    },
    "config": [
      {
        "variableName": "node_health",
        "displayName": "Node Health",
         "description": "The default health of the node",
        "type": "NUMBER",
        "defaultValue": "31"
      },
      {
        "variableName": "interact_script",
        "displayName": "Interact Script",
        "description": "The script to be run when the player interacts with the NPC",
        "type": "TEXT",
        "defaultValue": "cobblemon:iron_node"
      },
      {
        "variableName": "error_message",
        "displayName": "Error Message",
        "description": "The message to be displayed when the player does not have the required item",
        "type": "TEXT",
        "defaultValue": "§l§cYou need a pickaxe to mine this!"
      }
    ],
    "hideNameTag": true
}