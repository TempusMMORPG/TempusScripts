{
    "initializationAction": [
        "q.player.data.dialogue.regalis_keeper ? q.dialogue.set_page('farewell');",
        "q.player.data.regalis_grunt_3_1_defeated && q.player.data.regalis_grunt_3_2_defeated && q.player.data.regalis_grunt_3_3_defeated && q.player.data.regalis_grunt_3_4_defeated && q.player.data.regalis_grunt_3_5_defeated && q.player.data.regalis_grunt_3_6_defeated && q.player.data.regalis_grunt_3_7_defeated && q.player.data.regalis_grunt_3_8_defeated && q.player.data.regalis_grunt_3_9_defeated && q.player.data.regalis_grunt_3_10_defeated ? q.dialogue.set_page('greeting');"
    ],
    "speakers": {
        "npc": {
            "name": {
                "type": "expression",
                "expression": "q.npc.name"
            },
            "face": "q.npc.face();"
        },
        "player": {
            "face": "q.player.face();",
            "name": {
                "type": "expression",
                "expression": "q.player.username"
            }
        }
    },
    "pages": [
        {
            "id": "farewell",
            "speaker": "npc",
            "lines": [
                "..."
            ],
            "input": "q.dialogue.close();"
        },
        {
            "id": "greeting",
            "escapeAction": "0",
            "speaker": "npc",
            "lines": ["How did you get into our safe?!"],
            "input": "q.dialogue.set_page('1');"
        },
        {
            "id": "1",
            "escapeAction": "0",
            "speaker": "npc",
            "lines": ["That doesn't matter now. State your business here."],
            "input": {
                "type": "option",
                "vertical": false,
                "options": [
                    {
                        "text": "Hand over the family records!",
                        "value": "1",
                        "action": "q.dialogue.set_page('2');"
                    }
                ]
            }
        },
        {
            "id": "2",
            "escapeAction": "0",
            "speaker": "npc",
            "lines": ["...That's it? You just want the family records?"],
            "input": {
                "type": "option",
                "vertical": false,
                "options": [
                    {
                        "text": "Yes, that's all I want.",
                        "value": "1",
                        "action": "q.dialogue.set_page('3');"
                    }
                ]
            }
        },
        {
            "id": "3",
            "escapeAction": "0",
            "speaker": "npc",
            "lines": ["You're joking, right? You defeated all of our guards and broke into our treasured filled safe... just for the family records?"],
            "input": {
                "type": "option",
                "vertical": false,
                "options": [
                    {
                        "text": "...Yeah",
                        "value": "1",
                        "action": "q.dialogue.set_page('4');"
                    }
                ]
            }
        },
        {
            "id": "4",
            "escapeAction": "0",
            "speaker": "npc",
            "lines": ["You know what...? If this makes you leave faster, then here you go. You never got them from me."],
            "input": {
                "type": "option",
                "vertical": false,
                "options": [
                    {
                        "text": "...Thanks",
                        "value": "1",
                        "action": [
                            "q.run_command('ftbquests change_progress ' + q.player.username + ' complete 2D7E9483C649E231');",
                            "q.player.data.dialogue.regalis_keeper = true; q.player.save_data();",
                            "q.dialogue.close();"
                        ]
                    }
                ]
            }
        }
    ]
}