{
    "speakers": {
        "npc": {
            "name": {
                "type": "expression",
                "expression": "q.npc.name"
            },
            "face": "q.npc.face();",
            "gibber": {
                "interval": 0.1
            }
        },
        "player": {
            "face": "q.player.face();",
            "gibber": {
                "interval": 0.1
            },
            "name": {
                "type": "expression",
                "expression": "q.player.username"
            }
        }
    },
    "pages": [
        {
            "id": "teleport",
            "speaker": "npc",
            "lines": [
                "Hello there, you must be a new Trainer. Welcome to the Tempus Region! The Professor at the Lab in Unity City has asked for you. Would you like me to send you there now?"
            ],
            "input": {
                "type": "option",
                "vertical": true,
                "options": [
                    {
                        "text": "Go to Unity City Lab",
                        "value": "location1",
                        "action": [
                            "q.run_command('tp ' + q.player.username + ' 345 65 -707 -89 5.4');",
                            "q.run_command('ftbquests change_progress ' + q.player.username + ' complete 0B6A97171EA4BC42');",
                            "q.dialogue.close();",
                            "q.player.tell('§a§l[✔] Travelled to ' + q.npc.config.teleport_display_1 + '[✔]§r');"
                        ]
                    },
                    {
                        "text": "Exit",
                        "value": "exit",
                        "action": "q.dialogue.close();"
                    }
                ]
            }
        }
    ]
}