{
    "initializationAction": [
        "q.player.data.dialogue.sewer_worker == false || q.player.data.dialogue.grace ? q.dialogue.set_page('farewell');"
    ],
    "speakers": {
        "npc": {
            "name": {
                "type": "expression",
                "expression": "q.npc.name"
            },
            "face": "q.npc.face();"
        },
        "player": {
            "face": "q.player.face();",
            "name": {
                "type": "expression",
                "expression": "q.player.username"
            }
        }
    },
    "pages": [
        {
            "id": "greeting",
            "escapeAction": "0",
            "speaker": "npc",
            "lines": [
                "You're the trainer that Elon sent, right? I'm Grace. Conifer beat you here. He told me you were coming."
            ],
            "input": "q.dialogue.set_page('grace_one');"
        },
        {
            "id": "grace_one",
            "escapeAction": "0",
            "speaker": "npc",
            "lines": [
                 "Head down into the Crypt. Conifer is waiting for you there. He found something."
            ],
            "input": {
                "type": "option",
                "vertical": true,
                "options": [
                    {
                        "text": "Ok no problem",
                        "value": "okay",
                        "action": [
                            "q.run_command('ftbquests change_progress ' + q.player.username + ' complete 340D8AF86AB3FBF0');",
                            "q.player.data.dialogue.grace = true; q.player.save_data();",
                            "q.dialogue.set_page('farewell');"
                        ]
                    }
                        ]
                    }
            },
        {
            "id": "farewell",
            "speaker": "npc",
            "lines": [
                "See ya later!"
            ],
            "input": {
                "type": "option",
                "vertical": true,
                "options": [
                    {
                        "text": "Goodbye",
                        "value": "goodbye",
                        "action": [
                            "q.dialogue.close();"
                        ]
                    }
                        ]
                    }
        }
    ]
}