{
  "names": [
    "Smash Plugin"
  ],
  "resourceIdentifier": "cobblemon:flyingleader",
  "presets": [
    "cobblemon:base_npc"
  ],
  "interaction": {
    "type": "custom_script",
    "script": "c.player.tell(q.npc.name + ': ' + c.npc.config.dialogue);"
  },
  "config": [
      {
        "variableName": "dialogue",
        "displayName": "Dialogue",
        "description": "The dialogue for the NPC (Required)",
        "type": "TEXT",
        "defaultValue": "Hello!"
      },
      {
        "variableName": "resource",
        "displayName": "Resource Identifier",
        "description": "The resource for the NPC skin (Required)",
        "type": "TEXT",
        "defaultValue": "cobblemon:flyingleader"
      }
  ]
}