q.player.in_battle ? return 0;

t.props = q.npc.config.spawn_pokemon_props + ' lvl=' + q.npc.config.spawn_pokemon_level;
t.pokemon = q.player.world.spawn_pokemon(q.player.x, q.player.y, q.player.z, t.props);
