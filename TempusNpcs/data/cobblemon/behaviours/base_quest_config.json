{
  "name": "Quest NPC Config",
  "description": "To set up a quest NPC that can complete quests for players.",
  "entityType": "cobblemon:npc",
  "undo": [
    "q.entity.unset_interaction();"
  ],
  "configurations": [
    {
      "type": "custom_script",
      "variables": [
        {
          "variableName": "quest_id",
          "displayName": "Quest ID",
          "description": "The quest ID that this NPC is associated with.",
          "type": "TEXT",
          "defaultValue": "aaaaaaaaaaaaaaaa"
        },
        {
          "variableName": "complete_quest_command",
          "displayName": "Complete Quest Command",
          "description": "The MoLang that gets executed to complete the player's quest.",
          "type": "TEXT",
          "defaultValue": "q.run_command('ftbquests change_progress ' + q.player.username + ' complete ' + q.npc.config.quest_id);"
        }
      ],
      "script": [
      ]
    }
  ]
}
